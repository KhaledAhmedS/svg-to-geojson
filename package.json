{
  "name": "svg-to-geojson",
  "version": "0.0.1",
  "description": "Convert SVG to GeoJson",
  "author": "Dave Cranwell (@davecranwell)",
  "repository": "git://github.com/davecranwell/svg-to-geojson.git",
